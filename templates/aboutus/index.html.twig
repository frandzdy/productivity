{% extends 'layout.html.twig' %}

{% block title %}
    A propos, histoire, objectif de la société - {{ parent() }}
{% endblock %}

{% block metas %}
    {{ parent() }}
    <meta name="description" content="A propos, histoire, objectif de la société">
{% endblock %}

{% set breadcrumbItems =
    {
        'A propos': ''
    }
%}

{% block content %}

    <header class="masthead">
        <div class="container px-5">
            <div class="row align-items-center">
                <div class="col-lg-12">
                    {{ include('partials/_breadcrumbs.html.twig') }}
                </div>
            </div>
            <div class="row align-items-center">
                <div class="col-lg-12">
                   <h1>Qui sommes-nous ?</h1>
                </div>
            </div>
        </div>
    </header>
    {% include 'partials/_about_us.html.twig' %}
    {% include 'partials/_quote_about_us.html.twig' %}
    {% include 'partials/_about_us_conclusion.html.twig' %}
{% endblock %}